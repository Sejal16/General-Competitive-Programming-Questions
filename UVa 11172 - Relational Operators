import java.math.*;
import java.util.*;
public class Main
{
    static int j=0;
	public static void main(String[] args) {
	Scanner sc=new Scanner(System.in);
	
	int t=sc.nextInt();
	for(int i=0;i<t;i++)
	{
	    BigInteger q=sc.nextBigInteger();
	    BigInteger r=sc.nextBigInteger();
	    int res=q.compareTo(r);
	    
	    if(res==1)
	    System.out.println(">");
	    
	    else if(res==-1)
	    System.out.println("<");
	    
	    else
	    System.out.println("=");
	    
	}
	    
	}
}
