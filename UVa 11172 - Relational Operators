import java.util.*;
public class Main
{
    static int j=0;
	public static void main(String[] args) {
	Scanner sc=new Scanner(System.in);
	
	int t=sc.nextInt();
	for(int i=0;i<t;i++)
	{
	    int q=sc.nextInt();
	    int r=sc.nextInt();
	    
	    if(q>r)
	    System.out.println(">");
	    
	    else if(q<r)
	    System.out.println("<");
	    
	    else
	    System.out.println("=");
	    
	}
	    
	}
}
